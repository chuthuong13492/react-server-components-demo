  "name": "react-app",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=14.9.0"
  },
  "license": "MIT",
  "dependencies": {
    "@babel/core": "7.21.3",
    "@babel/plugin-transform-modules-commonjs": "^7.21.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/register": "^7.21.0",
    "@reduxjs/toolkit": "^2.9.1",
    "acorn-jsx": "^5.3.2",
    "acorn-loose": "^8.3.0",
    "babel-loader": "8.3.0",
    "compression": "^1.7.4",
    "concurrently": "^7.6.0",
    "express": "^4.18.2",
    "html-webpack-plugin": "5.5.0",
    "nodemon": "^2.0.21",
    "react": "18.3.0-next-1308e49a6-20230330",
    "react-dom": "18.3.0-next-1308e49a6-20230330",
    "react-redux": "^9.2.0",
    "react-server-dom-webpack": "18.3.0-next-1308e49a6-20230330",
    "resolve": "1.22.1",
    "rimraf": "^4.4.0",
    "webpack": "5.76.2"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.27.1",
    "@types/node": "^24.8.1",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "autoprefixer": "^10.4.21",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "postcss": "^8.5.6",
    "postcss-loader": "^8.2.0",
    "prettier": "1.19.1",
    "style-loader": "^4.0.0",
    "tailwindcss": "^4.1.14"
  },
  "scripts": {
    "start": "concurrently \"npm run server:dev\" \"npm run bundler:dev\"",
    "start:prod": "concurrently \"npm run server:prod\" \"npm run bundler:prod\"",
    "server:dev": "cross-env NODE_ENV=development nodemon -- --conditions=react-server server",
    "server:prod": "cross-env NODE_ENV=production nodemon -- --conditions=react-server server",
    "bundler:dev": "cross-env NODE_ENV=development nodemon -- scripts/build.js",
    "bundler:prod": "cross-env NODE_ENV=production nodemon -- scripts/build.js",
    "prettier": "prettier --write **/*.js"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-react",
        {
          "runtime": "automatic"
        }
      ],
      "@babel/preset-typescript"
    ]
  },
  "nodemonConfig": {
    "ignore": [
      "build/*"
    ]
  }
}
